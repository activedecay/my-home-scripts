[ui]
username = Justin Faust <justin.faust@sandisk.com>
merge = kdiff3

[paths]
cinder = ssh://hg//srv/hg/mgmt/cinder_ion
firehose = ssh://hg//srv/hg/firehose
management = ssh://hg//srv/hg/management
tools = ssh://hg//srv/hg/tools
mgmt_service = ssh://hg//srv/hg/mgmt/mgmt_service
mgmt_clients = ssh://hg//srv/hg/mgmt/mgmt_clients
flashcenter = ssh://hg//srv/hg/flashcenter
ioncli = ssh://hg//srv/hg/mgmt/ioncli
gargamel = ssh://hg//srv/hg/QA/gargamel

[extensions]
extdiff =
transplant =
progress =
graphlog =
record =
rebase =
purge =
color =
mq =
hgext.fusion = /hg/tools/hgext/fusion.py
reviewboard = /hg/tools/hgext/reviewboard
